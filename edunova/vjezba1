# Postavite cijenu na 10 svim
# proizvodima koji u svom nazivu
# nemaju slovo i

update products set buyprice=10
where productName not like '%m%';

# Izvucite ukupan broj proizvoda 
# koji imaju cijenu 20
select sum(1) from products
where buyPrice =20;


